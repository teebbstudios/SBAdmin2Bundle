<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="teebb_sbadmin2_redirect" path="/">
        <default key="_controller">Symfony\Bundle\FrameworkBundle\Controller\RedirectController::redirectAction</default>
        <default key="route">teebb_sbadmin2_dashboard</default>
        <default key="permanent">true</default>
    </route>

    <route id="teebb_sbadmin2_dashboard" path="/dashboard">
        <default key="_controller">Teebb\SBAdmin2Bundle\Controller\DashboardController</default>
    </route>


</routes>
